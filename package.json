{
  "name": "nuxtjs-bootstrap",
  "version": "1.0.0",
  "description": "My Nuxt.js bootstrap project",
  "author": "Riko Nagatama",
  "private": true,
  "scripts": {
    "dev-web": "cross-env NODE_ENV=development DEBUG=nuxt:* nodemon server/index.js --watch server",
    "dev-api": "cross-env NODE_ENV=development DEBUG=express:* nodemon api/index.js --watch api",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.5.3",
    "@nuxtjs/axios": "^5.4.1",
