    "@nuxtjs/pwa": "^2.6.0",
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "cookie": "^0.3.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "csrf": "^3.0.6",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "http-proxy-middleware": "^0.19.1",
    "jsonwebtoken": "^8.5.1",
    "nuxt": "^2.5.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
